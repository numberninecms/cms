{
    "name": "numberninecms/cms",
    "type": "symfony-bundle",
    "description": "NumberNine is a content management system for Symfony",
    "keywords": [
        "cms",
        "publishing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "William Arin",
            "email": "williamarin.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-sysvsem": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "doctrine/orm": "^2.7.4",
        "friendsofsymfony/jsrouting-bundle": "^2.6",
        "gedmo/doctrine-extensions": "^3.0",
        "imagine/imagine": "^1.2",
        "mpratt/relativetime": "^1.5",
        "numberninecms/common": "^0.1",
        "psr/cache": ">=1.0",
        "psr/log": "^1.1",
        "scienta/doctrine-json-functions": "^4.1",
        "sensio/framework-extra-bundle": "^5.1 || ^6.1",
        "stof/doctrine-extensions-bundle": "^1.4",
        "symfony-cmf/routing-bundle": "^2.4",
        "symfony/cache-contracts": "^2.4",
        "symfony/dependency-injection": "^5.3",
        "symfony/event-dispatcher": "^5.3",
        "symfony/form": "^5.3",
        "symfony/framework-bundle": "^5.3",
        "symfony/http-foundation": "^5.3",
        "symfony/http-kernel": "^5.3",
        "symfony/mailer": "^5.3",
        "symfony/mime": "^5.3",
        "symfony/options-resolver": "^5.3",
        "symfony/process": "^5.3",
        "symfony/rate-limiter": "^5.3",
        "symfony/routing": "^5.3",
        "symfony/security-bundle": "^5.3",
        "symfony/security-core": "^5.3",
        "symfony/security-csrf": "^5.3",
        "symfony/security-guard": "^5.3",
        "symfony/security-http": "^5.3",
        "symfony/serializer": "^5.3",
        "symfony/stopwatch": "^5.3",
        "symfony/string": "^5.3",
        "symfony/translation": "^5.3",
        "symfony/translation-contracts": "^2.0",
        "symfony/twig-bundle": "^5.3",
        "symfony/uid": "^5.3",
        "symfony/ux-turbo": "^1.3",
        "symfony/validator": "^5.3",
        "symfony/web-link": "^5.3",
        "symfony/web-profiler-bundle": "^5.3",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfonycasts/reset-password-bundle": "^1.3",
        "thunderer/shortcode": "^0.7.4",
        "twig/extra-bundle": "^3.0",
        "twig/twig": "^3.0",
        "wapmorgan/media-file": "^0.1.4"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.4",
        "ergebnis/composer-normalize": "^2.11",
        "friendsoftwig/twigcs": "^5.0",
        "nikic/php-parser": "^4.10",
        "numberninecms/chapterone": "dev-master",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpro/grumphp": "^1.0",
        "phpstan/phpstan": "^0.12.25",
        "phpstan/phpstan-doctrine": "^0.12.13",
        "phpstan/phpstan-phpunit": "^0.12.8",
        "phpstan/phpstan-symfony": "^0.12.6",
        "phpunit/phpunit": "^9.2",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^5.3",
        "symfony/console": "^5.3",
        "symfony/css-selector": "^5.3",
        "symfony/dotenv": "^5.3",
        "symfony/error-handler": "^5.3"
    },
    "suggest": {
        "numberninecms/chapterone": "Startup theme for NumberNine CMS"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "0.1.x-dev": "0.2.x-dev",
            "dev-master": "0.1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "NumberNine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NumberNine\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/grumphp run --no-interaction"
    }
}
