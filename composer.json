{
    "name": "numberninecms/cms",
    "description": "NumberNine is a content management system for Symfony",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "cms",
        "publishing"
    ],
    "authors": [
        {
            "name": "William Arin",
            "email": "williamarin.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-sysvsem": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/collections": "^1.6",
        "doctrine/common": "^3.1",
        "doctrine/data-fixtures": "^1.5",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-fixtures-bundle": "^3.4.1",
        "doctrine/migrations": "^3.3",
        "doctrine/orm": "^2.10",
        "friendsofsymfony/jsrouting-bundle": "^3.1",
        "gedmo/doctrine-extensions": "^3.0",
        "imagine/imagine": "^1.2",
        "mpratt/relativetime": "^1.5",
        "numberninecms/common": "^0.1.11",
        "psr/cache": ">=1.0",
        "psr/log": ">=1.1",
        "scienta/doctrine-json-functions": "^4.1",
        "sensio/framework-extra-bundle": "^5.1 || ^6.1",
        "stof/doctrine-extensions-bundle": "^1.4",
        "symfony-cmf/routing-bundle": "^3.0",
        "symfony/cache-contracts": "^2.4",
        "symfony/dependency-injection": "^6.0",
        "symfony/doctrine-bridge": "^6.0",
        "symfony/event-dispatcher": "^6.0",
        "symfony/expression-language": "^6.0",
        "symfony/form": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/mailer": "^6.0",
        "symfony/mime": "^6.0",
        "symfony/monolog-bundle": "^3.7",
        "symfony/options-resolver": "^6.0",
        "symfony/process": "^6.0",
        "symfony/rate-limiter": "^6.0",
        "symfony/routing": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/stopwatch": "^6.0",
        "symfony/string": "^6.0",
        "symfony/translation": "^6.0",
        "symfony/translation-contracts": "^2.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/uid": "^6.0",
        "symfony/ux-turbo": "^2.1",
        "symfony/validator": "^6.0",
        "symfony/web-link": "^6.0",
        "symfony/web-profiler-bundle": "^6.0",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfonycasts/reset-password-bundle": "^1.3",
        "thunderer/shortcode": "^0.7.4",
        "twig/extra-bundle": "^3.3",
        "twig/string-extra": "^3.3",
        "twig/twig": "^3.0",
        "wapmorgan/media-file": "^0.1.4"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.7",
        "ergebnis/composer-normalize": "^2.11",
        "friendsoftwig/twigcs": "dev-master",
        "nikic/php-parser": "^4.10",
        "numberninecms/chapterone": "^0.4",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpro/grumphp": "^1.0",
        "phpstan/phpstan": "^1.4.6",
        "phpstan/phpstan-doctrine": "^1.2.10",
        "phpstan/phpstan-phpunit": "^1.0.0",
        "phpstan/phpstan-symfony": "^1.1.5",
        "phpunit/phpunit": "^9.2",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/browser-kit": "^6.0",
        "symfony/console": "^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/error-handler": "^6.0",
        "symplify/easy-coding-standard": "^9.4"
    },
    "suggest": {
        "numberninecms/chapterone": "Startup theme for NumberNine CMS"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "NumberNine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NumberNine\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpro/grumphp": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    },
    "scripts": {
        "test": "vendor/bin/grumphp run --no-interaction"
    }
}
