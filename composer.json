{
    "name": "numberninecms/cms",
    "type": "symfony-bundle",
    "description": "NumberNine is a content management system for Symfony",
    "keywords": [
        "cms",
        "publishing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "William Arin",
            "email": "williamarin.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "doctrine/orm": "^2.7.4",
        "friendsofsymfony/jsrouting-bundle": "^2.6",
        "gedmo/doctrine-extensions": "^3.0",
        "imagine/imagine": "^1.2",
        "mpratt/relativetime": "^1.5",
        "numberninecms/common": "^0.1",
        "numberninecms/faker": "^0.1",
        "scienta/doctrine-json-functions": "^4.1",
        "stof/doctrine-extensions-bundle": "^1.4",
        "symfony-cmf/routing-bundle": "^2.4",
        "symfony/form": "^5.1",
        "symfony/framework-bundle": "^5.1",
        "symfony/http-kernel": "^5.1",
        "symfony/mime": "^5.1",
        "symfony/security-bundle": "^5.1",
        "symfony/security-core": "^5.1",
        "symfony/security-csrf": "^5.1",
        "symfony/security-guard": "^5.1",
        "symfony/security-http": "^5.1",
        "symfony/stopwatch": "^5.1",
        "symfony/string": "^5.1",
        "symfony/translation": "^5.1",
        "symfony/translation-contracts": "^2.0",
        "symfony/twig-bundle": "^5.1",
        "symfony/uid": "^5.1",
        "symfony/validator": "^5.1",
        "symfony/web-link": "^5.1",
        "symfony/web-profiler-bundle": "^5.1",
        "symfony/webpack-encore-bundle": "^1.7",
        "thunderer/shortcode": "^0.7.4",
        "twig/extra-bundle": "^3.0",
        "twig/twig": "^3.0",
        "wapmorgan/media-file": "^0.1.4"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.11",
        "friendsoftwig/twigcs": ">=4",
        "nikic/php-parser": "^4.10",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpro/grumphp": "^1.0",
        "phpstan/phpstan": "^0.12.25",
        "phpstan/phpstan-doctrine": "^0.12.13",
        "phpstan/phpstan-phpunit": "^0.12.8",
        "phpstan/phpstan-symfony": "^0.12.6",
        "phpunit/phpunit": "^9.2",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "NumberNine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NumberNine\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
